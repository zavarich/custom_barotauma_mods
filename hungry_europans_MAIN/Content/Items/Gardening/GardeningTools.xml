    <Item name="Hydroponic Planter" identifier="he-hydroponicplanter" tags="hydroponic,hydroponicplanter,donttakeitems,dontsellitems" category="Machine" subcategory="Gardening" linkable="true" allowedlinks="medcabinet,locker,kitchen" description="A device providing perfect conditions for europas underwater plants to grow. Yields a plant every 15 minutes. Needs power." isshootable="true" scale="0.3" damagedbyexplosions="true" explosiondamagemultiplier="0.2" canflipx="false">

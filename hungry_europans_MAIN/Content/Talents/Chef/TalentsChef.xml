  <!--Disable meat rotting in inventory-->
  <Override>
    <Talent identifier="he-coldblood">
      <Icon texture="%ModDir%/Content/UI/HE-IconsAtlas.png" sourcerect="905,905,119,119" origin="0.5,0.5" />

          <Conditional stun="gt 0" />
          <Conditional IsPlayer="true" />
          <Affliction identifier="he-stuffed" amount="0.1" />
          <Affliction identifier="he-sugarrich" amount="5" />
          <ReduceAffliction type="damage" amount="0.075" />
          <ReduceAffliction type="hunger" amount="0.1" />
          <ReduceAffliction type="thirst" amount="0.4" />
          <ReduceAffliction identifier="he-starvation" amount="20" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <!-- Regular eating -->
        <StatusEffect type="OnUse" target="This" Condition="-7">
          <Conditional InWater="false" />
        </StatusEffect>
        <!--Apply effects when eating-->
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Affliction identifier="he-sugarrich" amount="16.0" />
          <Affliction identifier="he-stuffed" amount="0.1" />
          <ReduceAffliction type="damage" amount="0.075" />
        </StatusEffect>
        <!--Play eating sound only if not contained within a waffle cone-->
        <StatusEffect type="OnUse" target="This">
          <Conditional InWater="false" />
          <Conditional HasTag="neq wafflecone" targetcontainer="true" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/EatingSoft.ogg" loop="True" range="200" volume="5.0" />
        </StatusEffect>
        <!--Decrease hunger when eating, only if needs are enabled-->
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Conditional hunger="gt 0.1" />
          <ReduceAffliction type="hunger" amount="0.1" />
        </StatusEffect>
        <!--Decrease thirst when eating, only if needs are enabled-->
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Conditional thirst="gt 0.4" />
          <ReduceAffliction type="thirst" amount="0.4" />
        </StatusEffect>
        <StatusEffect type="OnBroken" target="this">
          <Remove />
        </StatusEffect>
      </Holdable>
    </Item>

          <Conditional IsBot="true" />
          <Affliction identifier="he-stuffed" amount="1.43" />
          <Affliction identifier="he-sugarrich" amount="71.5" />
          <ReduceAffliction type="damage" amount="1.0725" />
          <ReduceAffliction type="hunger" amount="1.43" />
          <ReduceAffliction type="thirst" amount="5.72" />
          <ReduceAffliction identifier="he-starvation" amount="200" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <!-- Allow the item to be applied to a player fully, if the player is not in water and unconcious/stunned -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="0" setvalue="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional stun="gt 0" />
          <Conditional IsPlayer="true" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="14.3" comparison="And" multiplyafflictionsbymaxvitality="true">

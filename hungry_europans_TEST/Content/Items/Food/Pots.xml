    <Item hideinmenus="false" name="Craw Yum!" identifier="he-crawyumsoup" variantof="he-poisoneffectpreset_spoon" health="16" category="Misc" subcategory="Soups" Tags="smallitem,fooditem,foodbowl,spoonfood,soup" description="A soup." maxstacksize="1" maxstacksizecharacterinventory="1" cargocontaineridentifier="metalcrate" useinhealthinterface="false" scale="0.5" impactsoundtag="impact_bottle_glass" isshootable="false" requireaimtouse="false" impacttolerance="1">
      <PreferredContainer secondary="abandonedmedcab" spawnprobability="0.02" />
      <PreferredContainer secondary="abandonedcrewcab" minamount="0" maxamount="1" spawnprobability="0.02" />
      <PreferredContainer secondary="crewcab" amount="1" spawnprobability="0.0" notcampaign="True" />
      <PreferredContainer primary="table" secondary="kitchen,crewcab" />
      <InventoryIcon texture="%ModDir:2954998725%/Content/Items/Food/Bowls.png" sourcerect="0,0,72,72" origin="0.5,0.5" />
      <Sprite texture="%ModDir:2954998725%/Content/Items/Tools/Tools.png" sourcerect="342,0,45,20" depth="0.6" origin="0.5,0.5" />
      <Fabricate suitablefabricators="stove" requiredtime="3" requiresrecipe="false" amount="1">
        <RequiredSkill identifier="cooking" level="15" />
        <RequiredItem identifier="he-emptybowl" amount="1" usecondition="false" mincondition="0" />
        <RequiredItem identifier="he-potcrawyum" UseCondition="True" MinCondition="0.20" />
      </Fabricate>
      <Fabricate suitablefabricators="cafeteria" requiredtime="20" requiredmoney="49" fabricationlimitmin="0" fabricationlimitmax="7" />
      <Deconstruct time="1" requireddeconstructor="deconstructor" />
      <Body width="42" height="14" density="12" />
      <SuitableTreatment type="hunger" suitability="80" />
      <!-- <GreaterComponent canbeselected="false" canbepicked="false" allowingameediting="false" timeframe="0" /> -->
      <Holdable canbepicked="true" canBeCombined="false" removeOnCombined="false" slots="LeftHand,RightHand" holdangle="0" holdpos="36,-20" handle1="0,-5" handle2="0,-5" aimable="false" msg="ItemMsgPickUpSelect">
        <!-- EVERYTHING RELATED TO POISONING FOOD IS HANDLED VIA THE POISON EFFECT PRESET ITEMS DEFINED IN - variantof="" AND THEIR CODE IS LOCATED IN - "/Content/Items/#Presets/#ItemPresets.xml" -->
        <!--////////// BOT EATING \\\\\\\\\\-->
        <!-- Bots "eat" by holding the food in their hands -->
        <StatusEffect type="OnActive" target="This,Character" duration="1" condition="-1" stackable="false" comparison="And">
          <Conditional hunger="gt 1" targetcontainer="true" />
          <Conditional IsPlayer="false" targetcontainer="true" />
          <Conditional IsHuman="true" targetcontainer="true" />
          <Affliction identifier="he-fatrich" amount="15.0" />
          <Affliction identifier="ate_soup" amount="1" />
          <Affliction identifier="he-stuffed" amount="1" />
          <ReduceAffliction type="hunger" amount="1.25" />
          <ReduceAffliction type="thirst" amount="1.25" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/EatingSoft.ogg" loop="True" range="200" volume="5.0" />
        </StatusEffect>
        <!--////////// REGULAR EATING \\\\\\\\\\-->
        <!-- Use the item when it's held and the spoon in the other hand is used (indicated by applying a "usingspoon" tag on the character which this item checks for -->
        <StatusEffect type="OnActive" target="This" Condition="-1.0" disabledeltatime="true" stackable="false" comparison="And">
          <Conditional HasStatusTag="usingspoon" targetcontainer="true" />
          <Conditional HasStatusTag="neq emptyspoon" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" disabledeltatime="true" stackable="false" comparison="And" forceplaysounds="true">
          <Conditional HasStatusTag="usingspoon" />
          <Conditional HasStatusTag="neq emptyspoon" />
          <Conditional InWater="false" />
          <Affliction identifier="he-fatrich" amount="15.0" />
          <Affliction identifier="ate_soup" amount="1" />
          <Affliction identifier="he-stuffed" amount="1" />
          <ReduceAffliction type="hunger" amount="1.25" />
          <ReduceAffliction type="thirst" amount="1.25" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork1.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork2.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork3.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork4.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork5.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Slurping.ogg" loop="False" range="200" volume="4.0" selectionmode="Random" />
        </StatusEffect>
        <!-- Prevent using this item continuously by applying the tag "emptyspoon" which prevents the StatusEffect that uses the item from triggering again, effectively only allowing it to trigger once per mouse-click -->
        <StatusEffect type="OnActive" target="Character" tags="emptyspoon" interval="0.1" duration="0.2">
          <Conditional HasStatusTag="usingspoon" />
        </StatusEffect>
        <StatusEffect type="OnImpact" target="This" condition="-10" disabledeltatime="true" offset="0,10">
          <Sound file="Content/Sounds/Water/Splash1.ogg" range="500" volume="0.5" frequencymultiplier="2" selectionmode="random" />
          <Sound file="Content/Sounds/Water/Splash4.ogg" range="500" volume="0.5" frequencymultiplier="2" selectionmode="random" />
          <ParticleEmitter particle="whitegoosplash" colormultiplier="255,255,255,120" anglemin="80" anglemax="100" particleamount="3" velocitymin="100" velocitymax="200" scalemin="0.35" scalemax="0.6" />
        </StatusEffect>
        <StatusEffect type="OnBroken" target="This" disabledeltatime="true">
          <Remove />
          <SpawnItem identifier="he-dirtybowl" spawnposition="SameInventory" spawnifcantbecontained="true" SpawnIfNotInInventory="true" />
        </StatusEffect>
      </Holdable>
      <AiTarget sightrange="1000" static="true" />
    </Item>

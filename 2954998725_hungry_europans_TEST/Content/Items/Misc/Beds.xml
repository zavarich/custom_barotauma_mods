          <Conditional timeframe="gte 1" targetitemcomponent="GreaterComponent" />
          <Affliction identifier="he-sleep" amount="1" />
        </StatusEffect>
      </Controller>
      <GreaterComponent TimeFrame="0" canbeselected="false" canbepicked="false" allowingameediting="false" />

          <Conditional Quality="lte 0" />
          <Conditional HasTag="eq huskfooditem" />
          <SpawnItem identifier="huskeggsbasic" spawnposition="ThisInventory" spawnifinventoryfull="false" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" spawnifinventoryfull="false" />
          <SpawnItem identifier="effect_none" spawnposition="ThisInventory" spawnifinventoryfull="false" />
        </StatusEffect>
        <!-- rare chance to infect the character eating the item with worm infections, but only if hunger is enabled -->
        <StatusEffect type="OnUse" target="Character" interval="1" disabledeltatime="true" comparison="And">
          <RequiredItem identifier="parasiticworms" type="Contained" />
          <Conditional InWater="false" />
          <Conditional huskinfection="lt 50" />
          <Affliction identifier="worminfection" amount="1" probability="0.005" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="Character" interval="1" disabledeltatime="true" comparison="And">
          <RequiredItem identifier="tapeworm" type="Contained" />
          <Conditional InWater="false" />
          <Conditional huskinfection="lt 50" />
          <Affliction identifier="tapeworminfection" amount="1" probability="0.005" />
        </StatusEffect>
        <!--////////// POISON EFFECTS FOR REGULAR EATING \\\\\\\\\\-->

      <ItemComponent canbepicked="false" canbeselected="false" characterusable="true">
        <!-- spawn parasites inside the food if the food is low quality -->
        <StatusEffect type="OnContained" target="This" oneshot="true" spawnitemrandomly="true">
          <Conditional Quality="lte 0" />
          <SpawnItem identifier="parasiticworms" spawnposition="ThisInventory" spawnifinventoryfull="false" />
          <SpawnItem identifier="parasiticworms" spawnposition="ThisInventory" spawnifinventoryfull="false" />
